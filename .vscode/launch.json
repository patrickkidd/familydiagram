            "variablePresentation": {
                "all": "hide",
                "protected": "inline",
            },
            // "env": {
            //     "FD_LOG_LEVEL": "DEBUG",
            // }
            "args": [
                // "--windows-console"
            ],
            "env": {
                // "FD_SERVER_URL_ROOT": "http://127.0.0.1:8888",
            }
        },

        {
            "name": "app --version",
            "type": "debugpy",
            "request": "launch",
            "module": "pkdiagram",
            "console": "integratedTerminal",
            "justMyCode": false,
            "variablePresentation": {
                "all": "hide",
                "protected": "inline",
            },
            "args": [
                "--version"
            ],
            "env": {
                // "FD_SERVER_URL_ROOT": "http://127.0.0.1:8888",
            }
        },        
        {
            "name": "Personal",
            "type": "debugpy",
            "request": "launch",
            "module": "pkdiagram",
            "console": "integratedTerminal",
            "justMyCode": false,
            "variablePresentation": {
                "all": "hide",
                "protected": "inline",
            },
            "args": [
                "--personal"
            ],
            "env": {
                // "FD_SERVER_URL_ROOT": "http://127.0.0.1:8888",
            }
        },
        {
            "name": "App (prefs: 'free')",
            "type": "debugpy",
            "request": "launch",
            "module": "pkdiagram",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--prefs-name=free"
            ],
            "variablePresentation": {
                "all": "hide",
                "protected": "inline",
            }
        },
        {
            "name": "App (Windows)",
            "type": "debugpy",
            "request": "launch",
            "module": "pkdiagram",
            "console": "integratedTerminal",
            "justMyCode": false,
            "envFile": "/.vscode/.windows-env"
        },
        {
            "name": "sysroot-dev TestDialog",
            "type": "debugpy",
            "request": "launch",
            "program": "main_qml.py",
            "console": "integratedTerminal",
            "args": [
                "CopilotView"
            ],
            "env": {
                "--QT_LOGGING_RULES": "qt.quick.*=true"
            }
        },
        {
            "name": "ingest",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "console": "integratedTerminal",
            "justMyCode": false,
            "variablePresentation": {
                "all": "hide",
                "protected": "inline",
            },
            "args": [
                "ingest",
                "--data-dir",
                "${workspaceFolder}/tests/data/vector_db",
                "--sources-dir",
                "${workspaceFolder}/../btcopilot/btcopilot/tests/data"
            ],
            "env": {
                "FLASK_APP": "btcopilot",
            },
        },
        {
            "name": "python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "pyqtdeploy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/pyqtdeploy",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "pyqtdeploy-sysroot",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/pyqtdeploy-sysroot-debuggable.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--verbose",
                "--sysroots-dir",
                "${workspaceFolder}/sysroot",
                "sysroot/sysroot.toml"
            ]
        },
        {
            "name": "pyqtdeploy-sysroot (iPhoneSimulator)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/pyqtdeploy-sysroot-debuggable.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--target",
                "ios-64",
                "--force",
                "--verbose",
                "sysroot/sysroot.toml"
            ],
            "env": {
                "PK_IPHONE_SIMULATOR": "1",
            }
        },
        {
            "name": "pyqtdeploy-sysroot (iOS)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/pyqtdeploy-sysroot-debuggable.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--verbose",
                "--target",
                "ios-64",
                "--force",
                "sysroot/sysroot.toml"
            ]
        },
        {
            "name": "pyqtdeploy-build",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/pyqtdeploy-build-debuggable.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python",
            "justMyCode": false,
            "args": [
                "--verbose",
                "--resources",
                "12",
                "--target",
                "macos-64",
                "--build-dir",
                "build/osx",
                "familydiagram.pdt"
            ]
        },
        {
            "name": "update_build_info.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/update_build_info.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python",
            "justMyCode": false,
        },
        {
            "name": "sip-build (_pkdiagram)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/sip-build-debuggable.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/_pkdiagram"
        },
        {
            "name": "Debug Unit Test",
            "type": "debugpy",
            "purpose": "debug-test",
            "justMyCode": false,
            "skipFiles": [],
            "args": [
                "-svvx"
            ],
            "env": {
                // "QML_LOGGING": "qt.quick.mouse",
                // "QT_LOGGING_RULES": "qt.quick.*=true"
                // "QT_LOGGING_RULES": "qt.qpa.*=true"
            }
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/.direnv/python-3.7.8/bin/python", /* My virtual env */
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "MIDebuggerPath": "/usr/bin/lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(lldb) test_blow_up_ItemDetails_2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/python",
            "args": [
                "-m",
                "pytest",
                "-svv",
                "tests/test_addanythingdialog_scripts.py::test_blow_up_ItemDetails_2"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PYTHONPATH",
                    "value": "/Users/patrick/dev/fdserver"
                }
            ],
            "showAsyncStacks": "always",
            "maxAsyncDepth": 1000,
            "externalConsole": false,
            "MIMode": "lldb",
            "miDebuggerPath": "/Users/patrick/.vscode/extensions/ms-vscode.cpptools-1.21.6-darwin-x64/debugAdapters/lldb-mi/bin/lldb-mi",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                },
                {
                    "description": "Load Qt pretty-printers",
                    // "text": "command script import /Users/patrick/dev/familydiagram/lib/qt-everywhere-src-5.15.6/qtbase/src/corelib/debug_script.py",
                    "text": "command script import /Users/patrick/dev/familydiagram/lldb/qt_pretty_print.py",
                    "ignoreFailures": false
                },
                {
                    "description": "Load FD breakpoint",
